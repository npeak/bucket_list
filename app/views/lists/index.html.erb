<h1> Lists </h1>
<% @lists.each do |list|%>
    <h2><%= list.title%></h2>
    <%= link_to 'Show activites', list_activities_path(list)%>
    <br />
    <%= link_to 'Show destination', list_destinations_path(list)%>
    <br />
    <%= link_to 'Edit', edit_list_path(list) %>
    <br />
    <%= link_to 'Delete', list, method: :delete %>
<% end %>

<h1 strong > <%= link_to 'New list', new_list_path%> </h1>


